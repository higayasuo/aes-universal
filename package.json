{
  "name": "aes-universal",
  "version": "0.1.9",
  "description": "Universal AES encryption/decryption library that works on both web and native platforms",
  "private": false,
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "npm run typecheck && vite build",
    "test": "vitest --run",
    "coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm test && npm run build"
  },
  "keywords": [
    "expo",
    "crypto",
    "web",
    "universal",
    "encryption",
    "decryption",
    "aes",
    "hmac"
  ],
  "author": "Yasuo Higa",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/higayasuo/aes-universal.git"
  },
  "bugs": {
    "url": "https://github.com/higayasuo/aes-universal/issues"
  },
  "homepage": "https://github.com/higayasuo/aes-universal#readme",
  "devDependencies": {
    "@types/cbor-js": "^0.1.1",
    "@types/node": "^20.11.24",
    "typescript": "^5.2.2",
    "vite": "^5.1.4",
    "vite-plugin-dts": "^3.7.3",
    "vitest": "^1.3.1"
  },
  "peerDependencies": {
    "@noble/hashes": "^1.3.0",
    "u8a-utils": "^1.0.7"
  },
  "dependencies": {
    "cbor-js": "^0.1.0"
  }
}
